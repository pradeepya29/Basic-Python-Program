marks=int(input("enter the marks"))
if marks>=90:
    print("grade A")
    print("excellent")
elif marks>=70:
    print("grade B")
    print("good")
elif marks>=50:
    print("grade C")
    print("average")
elif marks>=40:
    print("grade D")
    print("poor")
else:
    print("grade F")
    print("fail")
